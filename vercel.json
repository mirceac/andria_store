{
        "version": 2,
        "builds": [
          {
            "src": "server/index.ts",
            "use": "@vercel/node"
          },
          {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
              "buildCommand": "npm run build",
              "outputDirectory": "dist"
            }
          }
        ],
        "routes": [
          {
            "src": "/api/(.*)",
            "dest": "/server/index.ts"
          },
          {
            "src": "/auth/(.*)",
            "dest": "/server/index.ts"
          },
          {
            "src": "/stripe-webhook",
            "dest": "/server/index.ts"
          },
          {
            "src": "/assets/(.*)",
            "dest": "/dist/assets/$1"
          },
          {
            "src": "/favicon.ico",
            "dest": "/dist/favicon.ico"
          },
          {
            "handle": "filesystem"
          },
          {
            "src": "/(.*)",
            "dest": "/dist/index.html"
          }
        ]
      }